#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( <http://purl.org/net/VideoGameOntology#Genre>
                bag:Item
                <http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection>
                giochi:Azienda
                giochi:Citta
                giochi:Difficolta
                giochi:Gioco
                giochi:ModalitaDiGioco
                giochi:Personaggio
                giochi:Piattaforma
                giochi:Portabilita
                <http://xmlns.com/foaf/0.1/Person>
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( giochi:AltraCitta
                giochi:CittaAmericana
                giochi:CittaGiapponese
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( giochi:CPU
                giochi:GPU
                giochi:RAM
                giochi:ROM
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( giochi:CasaProduttriceConsole
                giochi:CasaProduttriceGiocoDaTavolo
                giochi:CasaProduttricePC
              )
] .


[ rdf:type owl:AllDifferent ;
  owl:distinctMembers ( giochi:AaronGarbut
                        giochi:AlbertLamorisse
                        giochi:ImranSarwar
                        giochi:JamesTurner
                        giochi:JoeValenzuela
                        giochi:JunichiMasuda
                        giochi:KenSugimori
                        giochi:KenichiroYoshida
                        giochi:MaxRangel
                        giochi:PhilHooker
                        giochi:SamHouser
                        giochi:SatoshiTajiri
                        giochi:SatyaNadella
                        giochi:ShuntaruFurukawa
                        giochi:StanLee
                        giochi:SteveDitko
                        giochi:TedPrice
                      )
] .


