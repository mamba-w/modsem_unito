@prefix : <http://www.semanticweb.org/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix bag: <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix giochi: <http://www.semanticweb.org/giochi#> .
@prefix cpannotationschema: <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#> .
@prefix untitled-ontology-11: <http://www.semanticweb.org/matteotancredi/ontologies/2023/7/untitled-ontology-11#> .
@base <http://www.semanticweb.org/> .

<http://www.semanticweb.org/giochi> rdf:type owl:Ontology ;
                                     owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                     dc:creator "Matteo Tancredi"@it ;
                                     dc:date "12/09/2023" ;
                                     dc:description "Ontologia relativa ai giochi da tavolo e ai videogiochi, contente informazioni alle persone che li hanno creati, ai personaggi presenti, al genere di questi giochi e alle piattaforme a supporto."@it ;
                                     dc:title "Ontologia per gli appassionati di Giochi e Videogiochi"@it ;
                                     vann:PreferredNamespacePrefix "vgo" ;
                                     vann:PreferredNamespaceUri "http://www.semanticweb.org/giochi#" ;
                                     rdfs:label "Ontologia di Giochi e Videogiochi"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/PreferredNamespacePrefix
vann:PreferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/PreferredNamespaceUri
vann:PreferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem> rdf:type owl:ObjectProperty ;
                                                                 rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes> ;
                                                                 owl:inverseOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem> ;
                                                                 rdf:type owl:FunctionalProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem> rdf:type owl:ObjectProperty ;
                                                                     rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows> ;
                                                                     rdf:type owl:FunctionalProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows
<http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows> rdf:type owl:ObjectProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes
<http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes> rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/giochi#compostoDa
giochi:compostoDa rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain giochi:ComputerDaGaming ;
                  rdfs:range giochi:Lista_Pezzi_PC ;
                  rdfs:comment "un PC da gaming è composto di una lista di pezzi"@it ;
                  rdfs:label "è composto da"@it .


###  http://www.semanticweb.org/giochi#haCasaProduttriceConsole
giochi:haCasaProduttriceConsole rdf:type owl:ObjectProperty ;
                                owl:inverseOf giochi:produceConsole ;
                                rdfs:domain giochi:Console ;
                                rdfs:range giochi:CasaProduttriceConsole ;
                                rdfs:comment "Una console ha una casa produttrice"@it ;
                                rdfs:label "ha casa produttrice console"@it .


###  http://www.semanticweb.org/giochi#haCasaProduttriceGiocoDaTavolo
giochi:haCasaProduttriceGiocoDaTavolo rdf:type owl:ObjectProperty ;
                                      owl:inverseOf giochi:produceGiocoDaTavolo ;
                                      rdfs:domain giochi:GiocoDaTavolo ;
                                      rdfs:range giochi:CasaProduttriceGiocoDaTavolo ;
                                      rdfs:comment "Un gioco da tavolo ha una casa produttrice"@it ;
                                      rdfs:label "ha casa produttrice gioco da tavolo"@it .


###  http://www.semanticweb.org/giochi#haCasaProduttricePcGaming
giochi:haCasaProduttricePcGaming rdf:type owl:ObjectProperty ;
                                 owl:inverseOf giochi:producePcGaming ;
                                 rdfs:domain giochi:ComputerDaGaming ;
                                 rdfs:range giochi:CasaProduttricePC ;
                                 rdfs:comment "Un PC da gaming ha una casa produttrice"@it ;
                                 rdfs:label "ha casa produttrice PC gaming"@it .


###  http://www.semanticweb.org/giochi#haCreatoGiocoCon
giochi:haCreatoGiocoCon rdf:type owl:ObjectProperty ,
                                 owl:SymmetricProperty ;
                        rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                        rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                        rdfs:comment "una persona ha creato un gioco con un'altra persona"@it ;
                        rdfs:label "ha creato gioco con"@it .


###  http://www.semanticweb.org/giochi#haDifficolta
giochi:haDifficolta rdf:type owl:ObjectProperty ;
                    rdfs:domain giochi:Gioco ;
                    rdfs:range giochi:Difficolta ;
                    rdfs:comment "un gioco ha una difficoltà"@it ;
                    rdfs:label "ha difficoltà"@it .


###  http://www.semanticweb.org/giochi#haEtaMaggiore
giochi:haEtaMaggiore rdf:type owl:ObjectProperty ;
                     rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                     rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                     rdfs:comment "una persona è più grande di un'altra persona"@it ;
                     rdfs:label "ha età maggiore" .


###  http://www.semanticweb.org/giochi#haEtaMinore
giochi:haEtaMinore rdf:type owl:ObjectProperty ;
                   rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:comment "una persona è più piccola di un'altra persona"@it ;
                   rdfs:label "ha età minore"@it .


###  http://www.semanticweb.org/giochi#haGenere
giochi:haGenere rdf:type owl:ObjectProperty ;
                rdfs:domain giochi:Gioco ;
                rdfs:range <http://purl.org/net/VideoGameOntology#Genre> ;
                rdfs:comment "un gioco ha un genere"@it ;
                rdfs:label "ha genere"@it .


###  http://www.semanticweb.org/giochi#haModalita
giochi:haModalita rdf:type owl:ObjectProperty ;
                  rdfs:domain giochi:Gioco ;
                  rdfs:range giochi:ModalitaDiGioco ;
                  rdfs:comment "un gioco ha una modalità"@it ;
                  rdfs:label "ha modalità"@it .


###  http://www.semanticweb.org/giochi#haPersonaggio
giochi:haPersonaggio rdf:type owl:ObjectProperty ;
                     rdfs:domain giochi:Videogioco ;
                     rdfs:range giochi:Personaggio ;
                     rdfs:comment "un videogioco ha un personaggio"@it ;
                     rdfs:label "ha personaggio"@it .


###  http://www.semanticweb.org/giochi#haPiattaforma
giochi:haPiattaforma rdf:type owl:ObjectProperty ;
                     rdfs:domain giochi:Videogioco ;
                     rdfs:range giochi:Piattaforma ;
                     rdfs:comment "un videogioco ha una piattaforma"@it ;
                     rdfs:label "ha piattaforma"@it .


###  http://www.semanticweb.org/giochi#haPortabilita
giochi:haPortabilita rdf:type owl:ObjectProperty ;
                     rdfs:domain giochi:Console ;
                     rdfs:range giochi:Portabilita ;
                     rdfs:comment "una console ha una portabilità"@it ;
                     rdfs:label "ha portabilità"@it .


###  http://www.semanticweb.org/giochi#haSoftwareHouse
giochi:haSoftwareHouse rdf:type owl:ObjectProperty ;
                       owl:inverseOf giochi:produceVideogioco ;
                       rdfs:domain giochi:Videogioco ;
                       rdfs:range giochi:SoftwareHouse ;
                       rdfs:comment "un videogioco ha una software house"@it ;
                       rdfs:label "ha Software House"@it .


###  http://www.semanticweb.org/giochi#haStessaEta
giochi:haStessaEta rdf:type owl:ObjectProperty ;
                   rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                   rdfs:comment "due persone hanno la stessa età"@it ;
                   rdfs:label "ha stessa età"@it .


###  http://www.semanticweb.org/giochi#isAutoreDi
giochi:isAutoreDi rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                  rdfs:range giochi:Gioco ;
                  rdfs:comment "Una persona è autore di un gioco"@it ;
                  rdfs:label "è autore di"@it .


###  http://www.semanticweb.org/giochi#isCapoDi
giochi:isCapoDi rdf:type owl:ObjectProperty ;
                rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                rdfs:comment "una persona è capo di un'altra persona"@it ;
                rdfs:label "è capo di"@it .


###  http://www.semanticweb.org/giochi#isCeoDi
giochi:isCeoDi rdf:type owl:ObjectProperty ;
               rdfs:domain giochi:CEO ;
               rdfs:range giochi:Azienda ;
               rdfs:comment "una persona è CEO di una azienda"@it ;
               rdfs:label "è CEO di"@it .


###  http://www.semanticweb.org/giochi#isPersonaggioDi
giochi:isPersonaggioDi rdf:type owl:ObjectProperty ;
                       rdfs:domain giochi:Personaggio ;
                       rdfs:range giochi:Videogioco ;
                       owl:propertyChainAxiom ( giochi:isProtagonista
                                                giochi:isSecondario
                                                giochi:isVillain
                                              ) ;
                       rdfs:comment "Personaggio di un determinato videogioco"@it ;
                       rdfs:label "è personaggio di"@it .


###  http://www.semanticweb.org/giochi#isProtagonista
giochi:isProtagonista rdf:type owl:ObjectProperty ;
                      rdfs:domain giochi:Protagonista ;
                      rdfs:range giochi:Videogioco ;
                      rdfs:comment "un personaggio è personaggio prostagonista di un videogioco"@it ;
                      rdfs:label "è protagonista di"@it .


###  http://www.semanticweb.org/giochi#isSecondario
giochi:isSecondario rdf:type owl:ObjectProperty ;
                    rdfs:domain giochi:Secondario ;
                    rdfs:range giochi:Videogioco ;
                    rdfs:comment "un personaggio è personaggio secondario di un videogioco"@it ;
                    rdfs:label "è personaggio secondario di"@it .


###  http://www.semanticweb.org/giochi#isVillain
giochi:isVillain rdf:type owl:ObjectProperty ;
                 rdfs:domain giochi:Villain ;
                 rdfs:range giochi:Videogioco ;
                 rdfs:comment "un personaggio è personaggio villain di un videogioco"@it ;
                 rdfs:label "è villain di"@it .


###  http://www.semanticweb.org/giochi#lavoraDa
giochi:lavoraDa rdf:type owl:ObjectProperty ;
                rdfs:domain giochi:Sviluppatore ;
                rdfs:range giochi:SoftwareHouse ;
                rdfs:comment "uno sviluppatore lavora presso una softare house"@it ;
                rdfs:label "lavora da"@it .


###  http://www.semanticweb.org/giochi#lavoraPer
giochi:lavoraPer rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                 rdfs:range <http://xmlns.com/foaf/0.1/Person> ;
                 rdfs:comment "una persona lavora per un'altra persona"@it ;
                 rdfs:label "lavora per"@it .


###  http://www.semanticweb.org/giochi#produceConsole
giochi:produceConsole rdf:type owl:ObjectProperty ;
                      rdfs:domain giochi:CasaProduttriceConsole ;
                      rdfs:range giochi:Console ;
                      rdfs:comment "una casa produttrice produce una console"@it ;
                      rdfs:label "produce console"@it .


###  http://www.semanticweb.org/giochi#produceGiocoDaTavolo
giochi:produceGiocoDaTavolo rdf:type owl:ObjectProperty ;
                            rdfs:domain giochi:CasaProduttriceGiocoDaTavolo ;
                            rdfs:range giochi:GiocoDaTavolo ;
                            rdfs:comment "una casa produttrice produce un gioco da tavolo"@it ;
                            rdfs:label "produce gioco da tavolo"@it .


###  http://www.semanticweb.org/giochi#producePcGaming
giochi:producePcGaming rdf:type owl:ObjectProperty ;
                       rdfs:domain giochi:CasaProduttricePC ;
                       rdfs:range giochi:ComputerDaGaming ;
                       rdfs:comment "una casa produttrice produce un pc da gaming"@it ;
                       rdfs:label "produce PC da gaming"@it .


###  http://www.semanticweb.org/giochi#produceVideogioco
giochi:produceVideogioco rdf:type owl:ObjectProperty ;
                         rdfs:domain giochi:SoftwareHouse ;
                         rdfs:range giochi:Videogioco ;
                         rdfs:comment "una software house produce un videogioco"@it ;
                         rdfs:label "produce videogioco"@it .


###  http://www.semanticweb.org/giochi#ubicataIn
giochi:ubicataIn rdf:type owl:ObjectProperty ;
                 rdfs:domain giochi:Azienda ;
                 rdfs:range giochi:Citta ;
                 rdfs:comment "una azienda è ubicata in una città"@it ;
                 rdfs:label "è ubicata in"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/giochi#AnnoNascitaPersona
giochi:AnnoNascitaPersona rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf giochi:Data ;
                          rdfs:comment "data relativo all'anno di nascita di una persona"@it ;
                          rdfs:label "Anno di nascita di"@it .


###  http://www.semanticweb.org/giochi#AnnoUscita
giochi:AnnoUscita rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf giochi:Data ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( giochi:Gioco
                                              giochi:Piattaforma
                                            )
                              ] ;
                  rdfs:range xsd:int ;
                  rdfs:comment "data di uscita di un gioco"@it ;
                  rdfs:label "Uscita nel"@it .


###  http://www.semanticweb.org/giochi#Data
giochi:Data rdf:type owl:DatatypeProperty ;
            rdfs:comment "Data generica"@it ;
            rdfs:label "Data"@it .


###  http://www.semanticweb.org/giochi#FondataNel
giochi:FondataNel rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf giochi:Data ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( giochi:CasaProduttrice
                                              giochi:SoftwareHouse
                                            )
                              ] ;
                  rdfs:range xsd:integer ;
                  rdfs:comment "data relativa alla fondazione di una Azienda"@it ;
                  rdfs:label "Fondata nel"@it .


###  http://www.semanticweb.org/giochi#Modalita
giochi:Modalita rdf:type owl:DatatypeProperty ;
                rdfs:domain giochi:Gioco ;
                rdfs:range xsd:string ;
                rdfs:comment "modalità di gioco"@it ;
                rdfs:label "Modalita"@it .


###  http://www.semanticweb.org/giochi#Nome
giochi:Nome rdf:type owl:DatatypeProperty ;
            rdfs:comment "nome generico"@it ;
            rdfs:label "Nome"@it .


###  http://www.semanticweb.org/giochi#NomeCasaProduttrice
giochi:NomeCasaProduttrice rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf giochi:Nome ;
                           rdfs:domain [ rdf:type owl:Class ;
                                         owl:unionOf ( giochi:CasaProduttriceConsole
                                                       giochi:CasaProduttriceGiocoDaTavolo
                                                       giochi:CasaProduttricePC
                                                     )
                                       ] ;
                           rdfs:range xsd:string ;
                           rdfs:comment "nome di una casa produttrice"@it ;
                           rdfs:label "Nome casa produttrice"@it .


###  http://www.semanticweb.org/giochi#NomeCitta
giochi:NomeCitta rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf giochi:Nome ;
                 rdfs:range xsd:string ;
                 rdfs:comment "nome di una città"@it ;
                 rdfs:label "Nome città"@it .


###  http://www.semanticweb.org/giochi#NomeConsole
giochi:NomeConsole rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf giochi:Nome ;
                   rdfs:range xsd:string ;
                   rdfs:comment "nome di una console"@it ;
                   rdfs:label "Nome console"@it .


###  http://www.semanticweb.org/giochi#NomeGenere
giochi:NomeGenere rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf giochi:Nome ;
                  rdfs:range xsd:string ;
                  rdfs:comment "nome del genere del gioco"@it ;
                  rdfs:label "Nome genere"@it .


###  http://www.semanticweb.org/giochi#NomePcGaming
giochi:NomePcGaming rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf giochi:Nome ;
                    rdfs:range xsd:string ;
                    rdfs:comment "nome del PC da gaming"@it ;
                    rdfs:label "Nome PC da Gaming"@it .


###  http://www.semanticweb.org/giochi#NomePersona
giochi:NomePersona rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf giochi:Nome ;
                   rdfs:comment "nome di una persona"@it ;
                   rdfs:label "Nome della persona"@it .


###  http://www.semanticweb.org/giochi#NomePersonaggio
giochi:NomePersonaggio rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf giochi:Nome ;
                       rdfs:range xsd:string ;
                       rdfs:comment "nome di un Personaggio"@it ;
                       rdfs:label "Nome del personaggio"@it .


###  http://www.semanticweb.org/giochi#NomeSoftwareHouse
giochi:NomeSoftwareHouse rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf giochi:Nome ;
                         rdfs:domain giochi:SoftwareHouse ;
                         rdfs:range xsd:string ;
                         rdfs:comment "nome di una Software House"@it ;
                         rdfs:label "Nome della Software House"@it .


###  http://www.semanticweb.org/giochi#Portabilita
giochi:Portabilita rdf:type owl:DatatypeProperty ;
                   rdfs:domain giochi:Piattaforma ;
                   rdfs:range xsd:string .


###  http://www.semanticweb.org/giochi#TipoDifficolta
giochi:TipoDifficolta rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:string ;
                      rdfs:comment "tipo di difficoltà di un gioco"@it ;
                      rdfs:label "Tipo di difficoltà"@it .


###  http://www.semanticweb.org/giochi#Titolo
giochi:Titolo rdf:type owl:DatatypeProperty ;
              rdfs:domain giochi:Gioco ;
              rdfs:range xsd:string ;
              rdfs:comment "titolo di un gioco"@it ;
              rdfs:label "Titolo"@it .


#################################################################
#    Classes
#################################################################

###  http://purl.org/net/VideoGameOntology#Genre
<http://purl.org/net/VideoGameOntology#Genre> rdf:type owl:Class ;
                                              owl:equivalentClass [ rdf:type owl:Class ;
                                                                    owl:oneOf ( giochi:Avventura
                                                                                giochi:Picchiaduro
                                                                                giochi:RPG
                                                                                giochi:Rompicapo
                                                                              )
                                                                  ] ;
                                              rdfs:comment "Entita per descrivere il genere del gioco"@it ;
                                              rdfs:label "Genere"@it .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Bag
bag:Bag rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#Item
bag:Item rdf:type owl:Class .


###  http://www.semanticweb.org/giochi#AltraCitta
giochi:AltraCitta rdf:type owl:Class ;
                  rdfs:subClassOf giochi:Citta ;
                  rdfs:comment "Città generica dove è ubicata una casa produttrice"@it ;
                  rdfs:label "Altra Citta"@it .


###  http://www.semanticweb.org/giochi#Autore
giochi:Autore rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty giochi:isAutoreDi ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass giochi:Gioco
                                  ] ;
              rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
              owl:disjointUnionOf ( giochi:Disegnatore
                                    giochi:Ideatore
                                  ) ;
              rdfs:comment "Entita per descrivere un autore di un gioco o videogioco"@it ;
              rdfs:label "Autore"@it .


###  http://www.semanticweb.org/giochi#Azienda
giochi:Azienda rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty giochi:ubicataIn ;
                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass giochi:Citta
                                   ] ;
               rdfs:comment "Entita per descrivere una azienda produttrice di console, giochi e videogiochi"@it ;
               rdfs:label "Azienda"@it .


###  http://www.semanticweb.org/giochi#CEO
giochi:CEO rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty giochi:isCeoDi ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass giochi:Azienda
                               ] ;
           rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> ;
           owl:disjointUnionOf ( giochi:CEOCasaProduttrice
                                 giochi:CEOSoftwareHouse
                               ) ;
           rdfs:comment "CEO di una Azienda"@it ;
           rdfs:label "CEO"@it .


###  http://www.semanticweb.org/giochi#CEOCasaProduttrice
giochi:CEOCasaProduttrice rdf:type owl:Class ;
                          rdfs:subClassOf giochi:CEO ;
                          owl:disjointWith giochi:CEOSoftwareHouse ;
                          rdfs:comment "CEO di una casa produttrice"@it ;
                          rdfs:label "CEO Casa Produttrice"@it .


###  http://www.semanticweb.org/giochi#CEOSoftwareHouse
giochi:CEOSoftwareHouse rdf:type owl:Class ;
                        rdfs:subClassOf giochi:CEO ;
                        rdfs:comment "CEO di una Software House"@it ;
                        rdfs:label "CEO Software House"@it .


###  http://www.semanticweb.org/giochi#CPU
giochi:CPU rdf:type owl:Class ;
           rdfs:subClassOf giochi:PezziPC ;
           rdfs:comment "Processore del PC da gaming"@it ;
           rdfs:label "CPU"@it .


###  http://www.semanticweb.org/giochi#CasaProduttrice
giochi:CasaProduttrice rdf:type owl:Class ;
                       rdfs:subClassOf giochi:Azienda ;
                       owl:disjointWith giochi:SoftwareHouse ;
                       rdfs:comment "Casa produttrice di un gioco o console"@it ;
                       rdfs:label "Casa Produttrice"@it .


###  http://www.semanticweb.org/giochi#CasaProduttriceConsole
